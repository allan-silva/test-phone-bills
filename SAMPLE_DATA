# Closing bills

curl -X POST \
  http://127.0.0.1:8000/v1/bill/99988526423/close \
  -H 'content-type: application/json' \
  -d '{
  "month": 2,
  "year": 2016
}'

curl -X POST \
  http://127.0.0.1:8000/v1/bill/99988526423/close \
  -H 'content-type: application/json' \
  -d '{
  "month": 12,
  "year": 2017
}'

curl -X POST \
  http://127.0.0.1:8000/v1/bill/99988526423/close \
  -H 'content-type: application/json' \
  -d '{
  "month": 3,
  "year": 2018
}'
